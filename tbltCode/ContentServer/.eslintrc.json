{
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "no-underscore-dangle": "off",
    "no-unused-vars": "off",
    "arrow-body-style": "off",
    "no-param-reassign": "off",
    "max-len": "off"
  },
  "globals": {
    "log": true
  }
}
